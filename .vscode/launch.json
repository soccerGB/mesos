{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "mesos-tests (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/src/mesos-tests.exe",
            "args": [ "--gtest_filter=\"CommandExecutorCheckTest*\"" ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build/src",
            "environment": [],
            "externalConsole": false
        },
        {
            "name": "C++ Attach (Windows)",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}